#define _______ &trans

/ {
        keymap {
                compatible = "zmk,keymap";

                default_layer {
                        bindings = <
 &kp ESC,   &kp Q,   &kp W,   &kp E,   &kp R,   &kp T,                                    &kp Y,   &kp U,   &kp I,   &kp O,   &kp P,&kp BSLS,
&kp LCTL,   &kp A,   &kp S,   &kp D,   &kp F,   &kp G,                                    &kp H,   &kp J,   &kp K,   &kp L,&kp SCLN,&kp QUOT,
&mt SHIFT LPAR,   &kp Z,   &kp X,   &kp C,   &kp V,   &kp B,                                    &kp N,   &kp M,&kp COMM, &kp DOT,&kp SLSH,&mt SHIFT RPAR,
                                &kp LGUI,&lt L_LOWER TAB,&kp SPC,        &kp BSPC,&lt L_RAISE ENT,&kp RALT
   /* &kp TAB   &kp Q &kp W &kp E &kp R &kp T   &kp Y &kp U  &kp I     &kp O   &kp P    &kp BSPC */
   /* &kp LCTRL &kp A &kp S &kp D &kp F &kp G   &kp H &kp J  &kp K     &kp L   &kp SEMI &kp SQT */
   /* &kp LSHFT &kp Z &kp X &kp C &kp V &kp B   &kp N &kp M  &kp COMMA &kp DOT &kp FSLH &kp ESC */
   /*                &kp LGUI &mo 1 &kp SPACE   &kp RET &mo 2 &kp RALT */
                        >;
